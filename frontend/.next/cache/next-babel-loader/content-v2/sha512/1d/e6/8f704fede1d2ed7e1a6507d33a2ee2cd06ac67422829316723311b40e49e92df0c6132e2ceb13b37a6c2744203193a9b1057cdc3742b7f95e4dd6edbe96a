"import { createContext, useContext, useState } from 'react';\n\nconst LocalStateContext = createContext();\nconst LocalStateProvider = LocalStateContext.Provider;\n\nfunction CartStateProvider({ children }) {\n    //This is our own custom provider, we will store data and functionalilty in here\n\n    const [cartOpen, setCartOpen] = useState(false)\n\n    const openCart = () => {\n        setCartOpen(true)\n    }\n\n    return <LocalStateProvider value={{ cartOpen }}>{children}</LocalStateProvider>\n}\n\n// make a custom hook for accessing the cart local state\nfunction useCart() {\n    const all = useContext(LocalStateContext)\n    return all;\n}\n\nexport { CartStateProvider, useCart };"]},"metadata":{},"sourceType":"module"}